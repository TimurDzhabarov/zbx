                            </preprocessing>
                            <master_item>
                                <key>temperature.summary[{#ID}]</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>Upper NON critical Temperature of sensor {#NAME} ID: {#ID}  Celsius</name>
                            <type>DEPENDENT</type>
                            <key>upper.noncrit.celsius[{#ID}]</key>
                            <delay>0</delay>
                            <history>30d</history>
                            <value_type>FLOAT</value_type>
                            <applications>
                                <application>
                                    <name>Temperature</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.UpperThresholdNonCritical</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>temperature.summary[{#ID}]</key>
                            </master_item>
                        </item_prototype>
                    </item_prototypes>
                    <trigger_prototypes>
                        <trigger_prototype>
                            <expression>{BMC s2600wft:reading.celsius[{#ID}].last()}&gt;={BMC s2600wft:upper.crit.celsius[{#ID}].last()}</expression>
                            <name>Sensor {#NAME} CRITICALY heated up {ITEM.LASTVALUE}</name>
                            <priority>AVERAGE</priority>
                        </trigger_prototype>
                        <trigger_prototype>
                            <expression>{BMC s2600wft:reading.celsius[{#ID}].last()}&gt;={BMC s2600wft:upper.noncrit.celsius[{#ID}].last()}</expression>
                            <name>Sensor {#NAME} heated up {ITEM.LASTVALUE}</name>
                            <priority>WARNING</priority>
                        </trigger_prototype>
                    </trigger_prototypes>
                    <master_item>
                        <key>s2600wfttemperature.sh[{HOST.CONN},{$USR},{$PASS}]</key>
                    </master_item>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.Temperatures</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$PASS}</macro>
                    <value>password</value>
                </macro>
                <macro>
                    <macro>{$USR}</macro>
                    <value>user</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
